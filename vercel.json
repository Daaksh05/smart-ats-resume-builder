{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.py"
        },
        {
            "source": "/auth/:path*",
            "destination": "/api/index.py"
        },
        {
            "source": "/((?!api|auth).*)",
            "destination": "/index.html"
        }
    ]
}